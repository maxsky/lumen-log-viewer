    "license": "MIT",
    "keywords": ["log", "log-reader", "log-viewer", "logging", "lumen"],
    "type": "laravel-package",
    "authors": [
        {
            "name": "rap2hpoutre",
            "email": "raphaelht@gmail.com"
        },
        {
            "name": "Max Sky",
